{
	"version": "0.2.0",
	"configurations": [ 
		{
			"request": "launch",
			"name": "Launch Endpoint",
			"type": "node",
			"program": "https://cdn.unyt.org/uix/run.ts",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "/deno/latest/bin/deno",
			"runtimeArgs": [
				"run",
				"--no-check", "--importmap", "importmap.dev.json",
				"--allow-all"
			],
			"attachSimplePort": 9229,
			"args": [
				"--watch"
			],
			"console": "integratedTerminal"
		}
	]
}